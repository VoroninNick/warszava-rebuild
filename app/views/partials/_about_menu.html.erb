<ul id="about-us-nav">
  <li class="us-nav about-hide-element"><%= image_tag("crown-about.png") %></li>

  <% about_menu_obj = about_menu %>
  <% if about_menu_obj %>
      <% about_menu_items = about_menu_obj.menu_items %>
      <% about_menu_item_page_ids = [] %>
      <%# about_menu_items.each {|item| about_menu_item_page_ids.push(item.page.id) } %>

      <% about_menu_items.each do |item| %>
          <li class="us-nav about-line-up-menu">
          <% if page && page.id == item.page.id %>
              <%= image_tag("h1-mr.png", :alt => ">", :class => "nav-selector") %>
          <% else %>
            <div class="push-nav">&nbsp;</div>
          <% end %>

            <a href="<%= "/#{params[:locale]}#{item.page.path}" %>" title="<%= item.name %>"><%= item.name %></a>
          </li>
      <% end %>

  <% end %>

  <li class="us-nav about-hide-element"><%= image_tag("leavs-about.png") %></li>
</ul>